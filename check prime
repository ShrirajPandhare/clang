#include <stdio.h>
int CheckPrime(int i,int num)
{
	 if(num==i)
	return 0;
	else 
	if (num%i==0)
	return 1;
	else{
	
	return CheckPrime(i+1,num);
}
	}
	int main()
	{
	int num;
	
	printf("Enter a number to check ifprime ");
	scanf("%d",&num);
	
	if (CheckPrime(2,num)==0)
	
	
		printf(" It is a prime number.");
	
	
	else
 
	    printf("It is a composite number.");
	    
	}
	
	
	


    
